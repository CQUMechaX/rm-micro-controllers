#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rosidl-cli==2.2.1','console_scripts','rosidl'
__requires__ = 'rosidl-cli==2.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rosidl-cli==2.2.1', 'console_scripts', 'rosidl')()
    )
